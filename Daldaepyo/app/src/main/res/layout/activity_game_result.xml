<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:id="@+id/tv_result1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintWidth_percent="1"
        app:layout_constraintHeight_percent="0.085"
        android:background="@drawable/tv_rounded"
        android:textColor="@color/darkgray"
        android:gravity="center"
        android:text="주문 리스트"
        android:textSize="20sp"
        android:fontFamily="@font/suncheonr"
        android:textStyle="bold"/>
    <TextView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:id="@+id/tv_result2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintWidth_percent="1"
        app:layout_constraintHeight_percent="0.085"
        android:background="@drawable/tv_rounded"
        android:textColor="@color/darkgray"
        android:gravity="center"
        android:text="랜드마크"
        android:textSize="20sp"
        android:fontFamily="@font/suncheonr"
        android:textStyle="bold"/>

    <com.naver.maps.map.MapView
        android:id="@+id/map_view3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintWidth_percent="1"
        app:layout_constraintHeight_percent="0.5"
        app:layout_constraintVertical_bias="0.35">
    </com.naver.maps.map.MapView>

    <TextView
        android:id="@+id/tv_warning1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:fontFamily="@font/suncheonr"
        android:gravity="center"
        android:lineSpacingExtra="4dp"
        android:paddingLeft="7dp"
        android:text="대표자로 선정된 후 방을 나가면\n 패널티가 부여됩니다.\n\n 패널티 3회 이상 누적 시  \n일주일동안 사용이 중지됩니다."
        android:textColor="@color/red"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.15"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.8"
        app:layout_constraintWidth_percent="0.9" />

    <Button
        android:id="@+id/arrive"
        android:text="배달완료"
        android:textColor="@color/blue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintWidth_percent="0.65"
        app:layout_constraintHeight_percent="0.1"
        app:layout_constraintVertical_bias="0.95"
        app:layout_constraintHorizontal_bias="0.25" />

    <Button
        android:id="@+id/quit_game"
        android:text="게임 나가기"
        android:textColor="@color/blue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintWidth_percent="0.65"
        app:layout_constraintHeight_percent="0.1"
        app:layout_constraintVertical_bias="0.95"
        app:layout_constraintHorizontal_bias="0.75" />

    <Button
        android:id="@+id/take_quit"
        android:text="받았으면 나가기"
        android:textColor="@color/blue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintWidth_percent="0.65"
        app:layout_constraintHeight_percent="0.1"
        app:layout_constraintVertical_bias="0.95"
        app:layout_constraintHorizontal_bias="0.5" />



</androidx.constraintlayout.widget.ConstraintLayout>